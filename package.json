{
  "name": "sleekify",
  "version": "1.0.1",
  "description": "Typescript library that offers elegant and powerful utility functions",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "scripts": {
    "build": "npm run build:cjs && npm run build:esm && npm run build:types",
    "build:cjs": "tsc -p config/tsconfig.cjs.json",
    "build:esm": "tsc -p config/tsconfig.esm.json",
    "build:types": "tsc -p config/tsconfig.types.json",
    "test": "jest --runInBand",
    "coverage": "jest --coverage"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spiritmong3r/sleekify.git"
  },
  "author": "Cl√©ment Mouraut",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/spiritmong3r/sleekify/issues"
  },
  "homepage": "https://github.com/spiritmong3r/sleekify#readme",
  "dependencies": {
    "@stryker-mutator/core": "^6.0.2",
    "@stryker-mutator/jest-runner": "^6.0.2",
    "@types/jest": "^27.4.1",
    "benchmark": "^2.1.4",
    "microtime": "^3.1.0"
  },
  "devDependencies": {
    "@stryker-mutator/typescript-checker": "^6.0.2",
    "@types/benchmark": "^2.1.1",
    "jest": "^28.1.0",
    "ts-jest": "^28.0.2",
    "typescript": "^4.6.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "list",
    "util",
    "functional"
  ]
}
